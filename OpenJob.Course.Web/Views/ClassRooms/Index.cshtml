@model IEnumerable<OpenJob.Course.Web.Models.ClassRoomViewModels>

@{
    ViewBag.Title = "Index";
}
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.IdClassRoom }) |
                @Html.ActionLink("Details", "Details", new { id = item.IdClassRoom }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.IdClassRoom })
            </td>
        </tr>
    }

</table>